{
    "author": "devmoran",
    "name": "Noe dark",
    "semanticClass": "theme.dark",
    "colorSpaceName": "sRGB",
    "settings": [
        {
            "settings": {
                "background": "#1D252C",
                "foreground": "#A1A8B5",
                "invisibles": "#30343B",
                "lineHighlight": "#3c424c",
                "selection": "#3c424c"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "variable.language",
                "variable.other.member",
                "variable.other.readwrite",
                "support.variable.property",
                "support.variable.object.process",
                "meta.structure.dictionary.json",
                "string.quoted.double.json",
                "variable.other.readwrite",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#00C298"
            }
        },
        {
            "name": "Classes, types, SCSS properties, integers, imports",
            "scope": [
                "variable.parameter.function",
                "keyword.control",
                "variable.other.constant",
                "support.module.node",
                "support.type.object.module",
                "support.module.node",
                "entity.name.type.namespace",
                "entity.name.class.identifier.namespace.type",
                "support.class",
                "entity.name.type.class",
                "support.other.namespace",
                "entity.name.type",
                "entity.other.inherited-class",
                "meta.var.expr",
                "constant",
                "constant.numeric",
                "keyword.other.unit",
                "string.quoted.single",
                "string.quoted.double",
                "rgb-value",
                "var.this",
                "variable.language.this",
                "support.constant.property-value",
                "support.constant.color.w3c-extended-color-name",
                "support.type.primitive",
                "punctuation.definition.constant"
            ],
            "settings": {
                "foreground": "#FF80A4"
            }
        },
        {
            "name": "Punctuation",
            "comment": "All the punctuation",
            "scope": [
                "meta.brace",
                "punctuation",
                "meta.objectliteral",
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5C6370"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "entity.name.function",
                "variable.function",
                "meta.function-call.generic.python",
                "support.function.any-method",
                "support.function",
                "entity.other.attribute-name.id",
                "entity.name.class",
                "string.regexp",
                "constant.character.escape",
                "support.type.vendored.property-name"
            ],
            "settings": {
                "foreground": "#2399FA"
            }
        },
        {
            "name": "Storage",
            "scope": ["storage", "token.package.keyword"],
            "settings": {
                "foreground": "#c678dd"
            }
        },
        {
            "name": "Strings, Operators, supports",
            "scope": [
                "support.type.property-name",
                "keyword.operator",
                "string",
                "meta.object-literal.key",
                "token.package",
                "meta.tag"
            ],
            "settings": {
                "foreground": "#A1A8B5"
            }
        },
        {
            "name": "Keywords, braces, tags",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "entity.name.tag",
                "meta.function",
                "keyword",
                "keyword.control"
            ],
            "settings": {
                "foreground": "#CC4B76"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name",
                "meta.selector",
                "support.constant.color.w3c-standard-color-name",
                "support.constant.font-name",
                "entity.other.attribute-name.class",
                "support.constant.json.js",
                "function.parameter",
                "control.elements",
                "support.constant.json"
            ],
            "settings": {
                "foreground": "#DE8249"
            }
        },
        {
            "name": "Values",
            "scope": "none",
            "settings": {
                "foreground": "#DE8249"
            }
        },
        {
            "name": "Floats",
            "scope": "none",
            "settings": {
                "foreground": "#FF80A4"
            }
        },
        {
            "name": "Boolean",
            "scope": "none",
            "settings": {
                "foreground": "#FF80A4"
            }
        },
        {
            "name": "Delimiters",
            "scope": "none",
            "settings": {
                "foreground": "#5C6370"
            }
        },
        {
            "name": "Broken, illegal",
            "scope": ["invalid.broken", "invalid.illegal"],
            "settings": {
                "background": "#e05252",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "background": "#d27b53",
                "foreground": "#FFFFFF"
            }
        },
        {
            "name": "Unimplemented",
            "scope": "invalid.unimplemented",
            "settings": {
                "background": "#747369",
                "foreground": "#FFFFFF"
            }
        }
    ]
}
